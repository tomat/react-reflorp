{
  "extends": "eslint-config-rackt",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "valid-jsdoc": 2,
    "curly": 2,
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "react/wrap-multilines": 2,
    "semi": [2, "always"],
    "comma-dangle": 0
  },
  "plugins": [
    "react"
  ]
}
